    <script>
        function handleExternalURL() {
            $("a[target='_blank'][href*='____isexternal']").each(function (index, item) {                
                var href = $(item).attr("href");
                href = href.replace("&____isexternal=true", "");
                href = href.replace("?____isexternal=true", "");
                $(item).attr("href", href);
                if ($(item).closest(".no_external_url_indication,.footer_copyright").length <= 0) {
                    $(item).removeClass("external_link_url").addClass("external_link_url");
                }
            });

            $("a[target='_blank']:not([href*='____isexternal'])").each(function (index, item) {
                var linkUrl = $(item).attr("href");
                if ($.isUrl(linkUrl) && $(item).closest(".no_external_url_indication,.footer_copyright").length<=0) {
                    $(item).removeClass("external_link_url").addClass("external_link_url");
                }
            });
        }

        ; (function ($) {
            $.ajaxSetup({
                complete: function (XMLHttpRequest, textStatus) {                    
                    handleExternalURL();
                }
            });
        })(jQuery);
    </script>
